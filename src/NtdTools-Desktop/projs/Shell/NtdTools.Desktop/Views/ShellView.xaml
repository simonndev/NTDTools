<Window x:Class="NtdTools.Desktop.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
            prism:ViewModelLocator.AutoWireViewModel="True"
        xmlns:presentation="clr-namespace:NtdTools.Presentation;assembly=NtdTools.Presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NtdTools.Desktop.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ContentControl x:Name="ContentRoot"
                        prism:RegionManager.RegionName="{x:Static presentation:RegionNames.MainRegion}" />
    </Grid>
</Window>
